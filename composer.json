{
    "name": "proclame/laravel-notify-admin",
    "description": "An easy helper to notify the admin or webmaster about general events using laravel notification system",
    "license": "MIT",
    "require": {
        "illuminate/support": "^v8.0.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.1",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "suggest": {
        "laravel-notification-channels/telegram": "^0.5.1"
    },
    "autoload": {
        "psr-4": {
            "Proclame\\LaravelNotifyAdmin\\": "src/"
        },
        "files": [
            "src/notify_admin.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Proclame\\LaravelNotifyAdmin\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Nick Mispoulier",
            "email": "nick@proclame.be"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Proclame\\LaravelNotifyAdmin\\NotifyAdminServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
